{
  "name": "ws-dividend-tracker",
  "displayName": "WealthSimple Dividend Tracker",
  "version": "4.7.0",
  "description": "Track your dividends from WealthSimple and visualize your progress to financial independence!",
  "author": "CafDev Studio <caffein.dev@gmail.com>",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build --zip",
    "package": "plasmo package",
    "build:firefox": "plasmo build --target=firefox-mv2 --zip",
    "lint": "eslint 'src/**/*.{ts,tsx}' --fix"
  },
  "dependencies": {
    "@headlessui/react": "^2.1.8",
    "@heroicons/react": "^2.1.5",
    "@material-tailwind/react": "^2.1.8",
    "@plasmohq/messaging": "^0.6.2",
    "@plasmohq/storage": "^1.9.1",
    "@tanstack/react-query": "^5.28.14",
    "dexie": "^4.0.8",
    "lucide-react": "^0.456.0",
    "plasmo": "0.84.2",
    "query-string": "^9.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-select": "^5.8.1",
    "recharts": "^2.12.7",
    "tailwindcss": "3.4.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.42",
    "@types/react-dom": "18.2.18",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.0.0",
    "globals": "^15.0.0",
    "postcss": "8.4.33",
    "prettier": "3.2.4",
    "typescript": "5.3.3",
    "typescript-eslint": "^7.7.0"
  },
  "manifest": {
    "permissions": [
      "storage",
      "cookies"
    ],
    "host_permissions": [
      "*://*.wealthsimple.com/*",
      "*://*.finance.yahoo.com/*"
    ]
  }
}
